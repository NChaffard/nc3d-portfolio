---
interface Props {
	pageTitle: string;
}
const { pageTitle = "Portfolio NC3D" } = Astro.props;
---
<header class="header-container">
<h1 class="header-title">
    {pageTitle}
</h1>
</header>
<style>
    .header-container {
        width: 100%;
        height: 200px;
        display: flex;
        align-items: center;

    }
</style>